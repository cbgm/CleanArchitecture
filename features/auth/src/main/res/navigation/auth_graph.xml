<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/auth_graph"
   app:startDestination="@id/loginFragment"
   >

   <fragment android:id="@+id/loginFragment"
      android:name="com.distribution.christian.cleantest.auth.presentation.login.LoginFragment"
      android:label="LoginFragment"
      >
      <argument android:name="isUserLoggedIn"
         app:argType="boolean"
         android:defaultValue="false"
         />
      <action android:id="@+id/action_loginFragment_to_registerFragment"
         app:destination="@id/registerFragment"
         />
      <action android:id="@+id/action_loginFragment_to_resetFragment"
         app:destination="@id/resetFragment"
         />
   </fragment>
   <fragment android:id="@+id/registerFragment"
      android:name="com.distribution.christian.cleantest.auth.presentation.register.RegisterFragment"
      android:label="RegisterFragment"
      />
   <fragment android:id="@+id/resetFragment"
      android:name="com.distribution.christian.cleantest.auth.presentation.reset.ResetFragment"
      android:label="ResetFragment"
      />
</navigation>